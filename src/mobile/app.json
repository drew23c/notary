{
  "expo": {
    "name": "VC Mobile Signing Agent",
    "slug": "mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/writerHeader.png",
    "splash": {
      "image": "./assets/writerHeader.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.vcmobilesigningagent",
      "versionCode": 7
    },
    "packagerOpts": {
      "sourceExts": [ "js", "json", "ts", "tsx", "jsx", "vue"]
    }
  }
}
